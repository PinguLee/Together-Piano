{
  "name": "together-piano",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:server": "nodemon --watch '*.tsx' --exec ts-node --project tsconfig.server.json server/server.ts",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "npm run build:next && npm run build:server",
    "start": "node dist/server.js -p $PORT",
    "lint": "next lint"
  },
  "dependencies": {
    "@types/next-auth": "^3.15.0",
    "@types/webmidi": "^2.0.10",
    "express": "^4.18.2",
    "next": "^14.1.0",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "nodemon": "^3.0.3",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
